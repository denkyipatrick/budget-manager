{
  "manifest_version": 3,
  "name": "Budget Manager",
  "version": "1.0",
  "description": "This extension tracks your spending",
  "icons": {
    "128": "assets/images/icon128.png",
    "48": "assets/images/icon48.png",
    "16": "assets/images/icon16.png"
  },
  "action": {
    "defaul_icon": "assets/images/icon16.png",
    "default_popup": "index.html"
  },
  "options_page": "options.html",
  "background.service_worker": "event-page.js",
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ]
}